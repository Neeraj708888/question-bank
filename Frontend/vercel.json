{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/n"
    },
    {
      "src": "src/**/*",
      "use":"@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ], 
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}

